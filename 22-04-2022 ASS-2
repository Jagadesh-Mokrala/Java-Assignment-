class MOverload
{
    void area(float x, float y)
    {
        System.out.println("Area of rectangle "+x*y+" sq units");
    }
    void area(double x)
    {
        double z = 3.14 * x * x;
        System.out.println("Area of circle "+z+" sq units");
    }
}
class Overload 
{
     public static void main(String args[]) 
	{
	   MOverload ob = new MOverload();
	   ob.area(11,12);
	   ob.area(2.5);
        }
}
