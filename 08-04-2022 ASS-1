class Example
{
  public static void main(String args[]) 
  {
    int a=10,b=20;
    System.out.println("a is " + a + " and b is " + b);
    System.out.println(a == b);  
    System.out.println(a != b);  
    System.out.println(a > b);  
    System.out.println(a < b); 
    System.out.println(a >= b); 
    System.out.println(a <= b); 
  }
}

output:
    E:\>javac Examp.java

E:\>java Examp
a=10 and b=20
false
true
false
true
false
true

E:\>
