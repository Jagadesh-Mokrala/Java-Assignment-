
import java.util.*;
 
class Even
{
 
    public static void main(String args[])
    {
        int n=0,i=0;
         
        Scanner in = new Scanner(System.in);
         
        System.out.println("Enter value n : ");
        n = in.nextInt();
  
        for(i=1; i<n; i++)
        {
            if(i%2==0)
                System.out.print(i+" ");
        }  
    }
}
