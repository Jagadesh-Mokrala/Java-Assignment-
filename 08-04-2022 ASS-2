class Min
{
	public static void main(String args[])
	{
		System.out.print("Integer");
		System.out.print("\nMin value=" + Integer.MIN_VALUE);
		System.out.print("\nMax value=" + Integer.MAX_VALUE);
		System.out.print("\nFloat");
		System.out.print("\nMin value=" + Float.MIN_VALUE);
		System.out.print("\nMax value=" + Float.MAX_VALUE);
		System.out.print("\nByte");
		System.out.print("\nMin value=" + Byte.MIN_VALUE);
		System.out.print("\nMax value=" + Byte.MAX_VALUE);
		System.out.print("\nDouble");
		System.out.print("\nMin value=" + Double.MIN_VALUE);
		System.out.print("\nMax value=" + Double.MAX_VALUE);
		System.out.print("\nShort");
		System.out.print("\nMin value=" + Short.MIN_VALUE);
		System.out.print("\nMax value=" + Short.MAX_VALUE);
		System.out.print("\nLong");
		System.out.print("\nMin value=" + Long.MIN_VALUE);
		System.out.print("\nMax value=" + Long.MAX_VALUE);
	}
}

OUTPUT:
  E:\>javac Min.java

E:\>java Min
